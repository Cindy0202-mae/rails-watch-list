<%= render 'banner' %>
<div class="container mt-4">
  <h2>Watch Lists</h2>
  <div class="row">
    <% @lists.each do |list| %>
      <div class="col-md-4 mb-4">
        <div class="tile">
          <div class="tile-info">
            <h5><%= link_to list.name, list_path(list) %></h5>
          </div>
        </div>
      </div>
    <% end %>
  </div>
</div>
